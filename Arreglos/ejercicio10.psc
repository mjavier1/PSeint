Algoritmo sin_titulo
	Dimension a[5];
	a[1]=3;
	a[2]=5;
	a[3]=6;
	a[4]=8;
	a[5]=4;
	Dimension b[5];
	b[1]=3;
	b[2]=4;
	b[3]=6;
	b[4]=8;
	b[5]=1;
	
	r=0;
	Escribir "1. a[1]+b[2+1]=" Sin Saltar;
	r=a[1]+b[3];
	Escribir r;
	
	Escribir "2. a[3]mod b[2/2]=" Sin Saltar;
	r=a[3]mod b[1];
	Escribir r;
	
	Escribir "3. b[a[1]]-a[2]=" Sin Saltar;
	r=b[3]-a[2];
	Escribir r;
FinAlgoritmo
